@model Forestry_test.Models.Mazist
@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <h3>
        <a asp-area="" asp-controller="Mazists" asp-action="Index" class="btn btn-sm btn-warning">Назад</a>
    </h3>


@using (Html.BeginForm())
{
    @Html.LabelFor(model => model.FIO, "Имя")
